
public class RBTree {
	private Node root;
	
	private static final boolean RED = true;
	private static final boolean Black = false;
	
	private class Node {
		private Key key;
		private Value value;
		private boolean color;
		private int N;
		private Node left,right;
		
		public Node(Key key,Value value, boolean color, int N) {
			this.key = key;
			this.value = value;
			this.color = color;
		}
		
	} 
	
}
